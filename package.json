{
  "name": "plataforma-jzf",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently \"npm:dev:server\" \"npm:dev:client\"",
    "dev:client": "vite",
    "dev:server": "nodemon --watch 'server/**/*.ts' --exec \"ts-node --project server/tsconfig.json server/index.ts\"",
    "build:server": "tsc --project server/tsconfig.json",
    "build:client": "vite build",
    "build": "npm install && prisma generate && npm run build:client && npm run build:server",
    "start": "prisma migrate deploy && prisma db seed && node dist/server/index.js",
    "preview": "vite preview",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev --name init",
    "db:studio": "prisma studio",
    "db:seed": "prisma db seed"
  },
  "prisma": {
    "schema": "server/prisma/schema.prisma",
    "seed": "ts-node --project server/tsconfig.json server/prisma/seed.ts"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@google/genai": "^1.15.0",
    "@prisma/client": "5.15.0",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "express": "4.19.2",
    "jspdf": "2.5.1",
    "jspdf-autotable": "3.8.2",
    "marked": "12.0.2",
    "pdf-lib": "1.17.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "1.4.7",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/node": "20.12.12",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react": "4.3.0",
    "autoprefixer": "10.4.19",
    "concurrently": "8.2.2",
    "nodemon": "3.1.2",
    "prisma": "5.15.0",
    "tailwindcss": "3.4.3",
    "ts-node": "10.9.2",
    "typescript": "5.4.5",
    "vite": "5.2.12"
  }
}